    assign ratio_parts = aspect_ratio | split: '/'
    assign target_ratio = 1.0 | times: ratio_parts[0] | divided_by: ratio_parts[1]
  else 
    assign target_ratio = img.aspect_ratio
  endif

  # Calculate image dimensions
  if width
    assign width_2x = width | times: 2
    assign height_2x = width_2x | divided_by: target_ratio | round
    assign img_tag_width = width
    assign img_tag_height = width | divided_by: target_ratio | round
  elsif height
    assign height_2x = height | times: 2
    assign width_2x = height_2x | times: target_ratio | round
    assign img_tag_height = height
    assign img_tag_width = height | times: target_ratio | round
  endif

