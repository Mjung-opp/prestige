{% liquid
  assign slot = slot | default: ''
  assign as = as | default: 'div'
  assign id = id | default: ''
  assign classlist = classlist | default: '' | append: ' layout-stack' | strip
  assign horizontal = horizontal | default: false
  assign reverse = reverse | default: false
  assign wrap = wrap | default: 'wrap'
  assign justify = justify | default: ''
  assign align = align | default: ''
  assign attributes = attributes | default: ''
  assign style = style | default: ''

  assign gap = gap | default: 'none'
  assign gap_xs = gap_xs | default: gap
  assign gap_sm = gap_sm | default: gap
  assign gap_md = gap_md | default: gap
  assign gap_lg = gap_lg | default: gap
  assign gap_xl = gap_xl | default: gap

  if gap != 'none'
    assign style = style | append: '--layout-stack-gap: var(--gap-size-' | append: gap | append: ');'
  endif

  if gap_xs != gap
    assign style = style | append: '--layout-stack-gap--xs: var(--gap-size-' | append: gap_xs | append: ');'
  endif

  if gap_sm != gap
    assign style = style | append: '--layout-stack-gap--sm: var(--gap-size-' | append: gap_sm | append: ');'
  endif

  if gap_md != gap
    assign style = style | append: '--layout-stack-gap--md: var(--gap-size-' | append: gap_md | append: ');'
  endif

  if gap_lg != gap
    assign style = style | append: '--layout-stack-gap--lg: var(--gap-size-' | append: gap_lg | append: ');'
  endif

  if gap_xl != gap
    assign style = style | append: '--layout-stack-gap--xl: var(--gap-size-' | append: gap_xl | append: ');'
  endif

  if justify != blank
    assign style = style | append: ' --layout-stack-justify: ' | append: justify | append: ';'
  endif

  if align != blank
    assign style = style | append: ' --layout-stack-align: ' | append: align | append: ';'
  endif

  if horizontal
    if reverse
      assign style = style | append: ' --_layout-stack-direction: row-reverse;'
    else
      assign style = style | append: ' --_layout-stack-direction: row;'
    endif
  else
    if reverse
      assign style = style | append: ' --_layout-stack-direction: column-reverse;'
    else
      assign style = style | append: ' --_layout-stack-direction: column;'
    endif
  endif

  if wrap == 'nowrap' or wrap == 'wrap-reverse'
    assign style = style | append: ' --layout-stack-wrap: ' | append: wrap | append: ';'
  endif
%}

